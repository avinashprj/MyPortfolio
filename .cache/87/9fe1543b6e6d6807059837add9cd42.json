Poppins', sans-serif;\n    margin:auto;\n    max-width: 90%;\n    box-sizing: border-box;\n\n}\n\n\nhr {\n    margin: 2rem 0rem\n}\n\n/** container**/\n\n\n.container-center {\n    max-width: 600px;\n    margin: auto;\n}\n\n/**links**/\n\n .links {\n    text-decoration: none;\n    color: var(--button-text-color);\n    text-transform: capitalize;\n\n}\n.linksSymbol{\n    padding: 0.5rem 1rem;\n    font-size: xx-large;\n}\n\n\n\n.linksSymbol :hover{\n    transform:translate(0,-5px);\n\n    font-size: 2.5rem;\n}\n\n\n\n.link-primary {\n    background-color: var(--primary-color);\n    border-radius: 2rem;\n    color: white;\n    padding: var(--padding);\n\n}\n\n.link-secondary {\n    color: var(--primary-color);\n    border-radius: 2rem;\n    padding: var(--padding);\n    border: 1px solid var(--primary-color);\n\n}\n\n.fa{\n    transition: .6s;\n}\n\n\n\n\n/* lists */\n\n.list-non-bullet {\n    list-style-type: none;\n}\n\n.list-item-inline {\n    display: inline;\n    padding: 0rem 0.5rem;\n}\n\n\n\n.navigation {\n\n    background-color: var(--primary-color);\n    color: white;\n    padding: 1rem 1rem;\n    border-bottom-left-radius: 1rem;\n\n\n}\n\n\n.navigation .nav-brand {\n    font-weight: bold;\n    color:whitesmoke;\n    font-size: 1.1rem;\n\n}\n.navigation li {\n    display: inline;\n    margin: 0 1rem;\n}\n.navigation li :hover{\n    border-bottom:3px solid white !important;\n}\n\n.navigation .nav-pills {\n    text-align: right;\n}\n\n\n\n.navigation .link-active {\n    font-weight: bolder;\n}\n\n/** header **/\n\n.hero {\n    padding: 2rem;\n}\n\n.hero .hero-image {\n    max-width: 400px;\n    display: block;\n    margin: auto;\n    width:100%;\n    transition: 0.5s ease;\n}\n\n.hero .hero-heading {\n    text-align: center;\n    color: var(--dark-gray);\n    padding-top: 1rem;\n}\n\n\n/**sections **/\n\n.section {\n    padding: 2rem;\n    text-align:center;\n    margin-bottom:50px\n}\n\n.section.ow {\n    background-color: var(--off-white);\n    padding-bottom: 2rem;\n    border-radius:2rem;\n    transition: 0.5s ease;\n    max-width:80%;\n    margin:10px auto;\n}\n\n\n/**footer**/\n.footer{\n    background-color: var(--primary-color);\n    padding:.5rem 1rem;\n    text-align: center;\n    color: white;\n    border-top-right-radius: 1rem;\n    margin-top: 80px;\n\n\n}\n.footer-bottom{\n   margin-top: 20rem;\n\n}\n\n\n/* .footer ul{\n    padding-inline-start:0px;\n} */\n   .footer .footer-header{\n    font-weight: bold;\n    font-size: large;\n}\n\n\n/* .footer-list li {\n    display: inline;\n    margin: 0 0.5rem;\n} */\n\n.weak{\n    font-weight: lighter;\n}\n\n\n\n\n\n\n/* Media queries */\n\n@media(max-width:500px){\n\n   .hero .hero-image{\n       max-width: 350px;\n   }\n    .navigation ul{\n      padding-inline-start: 20px;\n\n    }\n\n\n\n}\n@media(max-width:400px){\n\n\n    .flex-link{\n        display:flex;\n\n         width: 151px;\n          margin-bottom: 10px;\n       text-transform: uppercase;\n    }\n    .navigation ul{\n      padding-inline-start: 10px;\n\n    }\n\n\n\n}\n"},"lineCount":243}},"error":null,"hash":"a01743a51efd2f7c5ebbe64d2d71a118","cacheData":{"env":{}}}